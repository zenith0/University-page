  <circle cx="420" cy="360" r="5" fill="#FFC107"/>
  
  <!-- Roof -->
  <polygon points="200,150 400,80 600,150" fill="#C62828"/>
  
  <!-- Sign "Residence Hall" -->
  <rect x="300" y="120" width="200" height="40" fill="#FFFFFF" stroke="#424242" stroke-width="2"/>
  <text x="340" y="145" font-family="Arial" font-size="18" font-weight="bold" fill="#424242">RESIDENCE HALL</text>
  
  <!-- Trees -->
  <rect x="100" y="350" width="25" height="50" fill="#6D4C41"/>
  <circle cx="112" cy="340" r="35" fill="#66BB6A"/>
  
  <rect x="675" y="350" width="25" height="50" fill="#6D4C41"/>
  <circle cx="687" cy="340" r="35" fill="#66BB6A"/>
  
  <!-- Pathway -->
  <polygon points="350,400 450,400 480,600 320,600" fill="#9E9E9E"/>
  
  <!-- Benches -->
  <rect x="120" y="420" width="60" height="10" fill="#8D6E63"/>
  <rect x="125" y="430" width="10" height="20" fill="#6D4C41"/>
  <rect x="165" y="430" width="10" height="20" fill="#6D4C41"/>
  
