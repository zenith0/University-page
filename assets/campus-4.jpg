  <!-- Dormitory building -->
  <rect x="200" y="150" width="400" height="250" fill="#EF5350"/>
  
  <!-- Windows grid -->
  <rect x="230" y="180" width="50" height="50" fill="#FFF9C4"/>
  <rect x="300" y="180" width="50" height="50" fill="#FFF9C4"/>
  <rect x="370" y="180" width="50" height="50" fill="#FFF9C4"/>
