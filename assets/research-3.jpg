  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#medBg)"/>
  
  <!-- Lab equipment -->
  <rect x="150" y="350" width="500" height="200" fill="#90A4AE"/>
  
  <!-- Microscope -->
  <ellipse cx="250" cy="320" rx="40" ry="15" fill="#546E7A"/>
  <rect x="235" y="320" width="30" height="100" fill="#607D8B"/>
  <rect x="220" y="280" width="60" height="40" fill="#78909C"/>
  <circle cx="250" cy="260" r="25" fill="#455A64"/>
  
