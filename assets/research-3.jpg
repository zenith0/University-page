  <rect x="450" y="280" width="30" height="120" rx="15" fill="#E3F2FD" stroke="#1976D2" stroke-width="3"/>
  <rect x="450" y="330" width="30" height="70" fill="#4CAF50" opacity="0.6"/>
  
  <rect x="500" y="280" width="30" height="120" rx="15" fill="#E3F2FD" stroke="#1976D2" stroke-width="3"/>
  <rect x="500" y="340" width="30" height="60" fill="#FF5722" opacity="0.6"/>
  
