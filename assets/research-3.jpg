    <path d="M 40 0 Q 0 20 -40 40 Q 0 60 40 80 Q 0 100 -40 120" stroke="#673AB7" stroke-width="4" fill="none"/>
  </g>
  
  <!-- Medical cross -->
  <rect x="100" y="100" width="30" height="100" fill="#F44336"/>
  <rect x="70" y="130" width="90" height="30" fill="#F44336"/>
  
  <!-- Molecule structure -->
