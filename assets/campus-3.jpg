<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <defs>
    <linearGradient id="libBg" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FFF8E1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFECB3;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#libBg)"/>
  
  <!-- Floor -->
  <rect y="450" width="800" height="150" fill="#BCAAA4"/>
  
  <!-- Bookshelves -->
  <rect x="50" y="200" width="120" height="250" fill="#6D4C41"/>
  <rect x="200" y="200" width="120" height="250" fill="#6D4C41"/>
  <rect x="480" y="200" width="120" height="250" fill="#6D4C41"/>
  <rect x="630" y="200" width="120" height="250" fill="#6D4C41"/>
  
  <!-- Books on shelves -->
  <!-- Shelf 1 -->
  <rect x="55" y="210" width="15" height="40" fill="#E53935"/>
  <rect x="72" y="210" width="12" height="40" fill="#1E88E5"/>
  <rect x="86" y="210" width="18" height="40" fill="#43A047"/>
  <rect x="106" y="210" width="14" height="40" fill="#FB8C00"/>
  <rect x="122" y="210" width="16" height="40" fill="#8E24AA"/>
  
  <rect x="55" y="270" width="14" height="40" fill="#00ACC1"/>
  <rect x="71" y="270" width="16" height="40" fill="#C62828"/>
  <rect x="89" y="270" width="13" height="40" fill="#6A1B9A"/>
  <rect x="104" y="270" width="17" height="40" fill="#2E7D32"/>
  <rect x="123" y="270" width="15" height="40" fill="#F57C00"/>
  
  <!-- Reading table -->
  <ellipse cx="400" cy="420" rx="150" ry="40" fill="#8D6E63"/>
  <rect x="250" y="400" width="300" height="20" fill="#A1887F"/>
  
  <!-- Laptop on table -->
  <rect x="340" y="370" width="120" height="80" fill="#37474F"/>
  <rect x="350" y="380" width="100" height="60" fill="#64B5F6"/>
  <line x1="360" y1="390" x2="440" y2="390" stroke="#1976D2" stroke-width="2"/>
  <line x1="360" y1="405" x2="420" y2="405" stroke="#1976D2" stroke-width="2"/>
  <line x1="360" y1="420" x2="430" y2="420" stroke="#1976D2" stroke-width="2"/>
  
  <!-- Books on table -->
  <rect x="270" y="385" width="40" height="30" fill="#D32F2F"/>
  <rect x="275" y="380" width="40" height="30" fill="#1976D2"/>
  
  <!-- Lamp -->
  <rect x="520" y="360" width="10" height="60" fill="#455A64"/>
  <polygon points="500,360 540,360 530,330 510,330" fill="#FDD835"/>
  <line x1="515" y1="350" x2="525" y2="350" stroke="#FBC02D" stroke-width="3"/>
  
  <!-- Window -->
  <rect x="350" y="80" width="100" height="100" fill="#81D4FA"/>
  <line x1="350" y1="130" x2="450" y2="130" stroke="#0277BD" stroke-width="3"/>
  <line x1="400" y1="80" x2="400" y2="180" stroke="#0277BD" stroke-width="3"/>
  
  <!-- Person studying -->
  <circle cx="400" cy="340" r="20" fill="#FF7043"/>
  <line x1="400" y1="360" x2="400" y2="400" stroke="#FF7043" stroke-width="5"/>
  <line x1="400" y1="370" x2="380" y2="395" stroke="#FF7043" stroke-width="5"/>
  <line x1="400" y1="370" x2="420" y2="395" stroke="#FF7043" stroke-width="5"/>
</svg>
